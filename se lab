#include<stdio.h>
#include<math.h>
int main()
{
    int t=30;
    int h=75;
    int w=25;
    double y=0;
    y=0.5*pow(t,2)-0.2*h+0.1*w-15;
    if(y>300)
    printf("sunny");
    else if(200<y && y<=300)
    printf("cloudy");
    else if(100<y && y<=200)
    printf("rainy");
    else if(y<100)
    printf("strom");
    return 0;
}
